    <workshops id="1125" key="ed5a14dc-a30a-4f10-a61a-6127a90d4c06" parentID="1121" level="2" creatorID="0" sortOrder="3" createDate="2017-11-21T15:39:04" updateDate="2017-11-21T16:32:53" nodeName="Workshops" urlName="workshops" path="-1,1121,1125" isDoc="" nodeType="1116" creatorName="Alexander Nielsen" writerName="Alexander Nielsen" writerID="0" template="1115" nodeTypeAlias="workshops">
